# Simulation of AI Agents in Smart Manufacturing
import matplotlib.pyplot as plt

# Sample defect data over 10 batches
batches = list(range(1,11))
defect_rates = [0.12, 0.10, 0.15, 0.08, 0.20, 0.14, 0.09, 0.11, 0.13, 0.07]

plt.plot(batches, defect_rates, marker='o', label='Defect Rate')
plt.axhline(y=0.05, color='r', linestyle='--', label='Threshold')
plt.xlabel('Batch Number')
plt.ylabel('Defect Rate')
plt.title('Quality Control Agent Monitoring')
plt.legend()
plt.show()
